			<texPath>Things/Meals/SoupFine_Veg</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
		</graphicData>
		<statBases>
			<MarketValue>20</MarketValue>
			<Nutrition>0.9</Nutrition>
