	<ThingDef ParentName="RoughPlantBase">
		<defName>VCE_UncookedSoupFine_Veg</defName>
		<label>uncooked vegetarian fine soup</label>
		<description>An uncooked vegetarian fine soup. This needs to be cooked in an electric pot to be edible.</description>
		<graphicData>
			<texPath>Things/Items/Uncooked/UncookedSoupFine_Veg</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<statBases>
			<MarketValue>136.4</MarketValue>
			<Flammability>1.3</Flammability>
			<WorkToMake>1800</WorkToMake>
			<Mass>4</Mass>
		</statBases>
		<thingSetMakerTags />
		<tradeability>None</tradeability>
